package com.qa.singleResponsibility;

public class Driver {
    private int mileage;

    // functionality
    public void drive(String milesDriven){
        this.setMileage(this.getMileage() + milesDriven);
    }

	private void setMileage(int mileage) {
		this.mileage = mileage;
		
	}

	private int getMileage() {
		return mileage;
	}
}