package garageChallenge;

public abstract class Vehicle {
	
	// Attributes
	Integer VehicleID;
	Integer Wheels;
	String Colour;
	Integer ManufactureDate;
	
	// Methods
	public abstract Double getAge();
	public abstract Double costOfWork();
	
	// Class Constructors
	public Vehicle() {
	}
	
	public Vehicle (Integer VehicleID, Integer Wheels, String Colour, Integer ManufactureDate) {
		this.VehicleID = VehicleID;
		this.Wheels = Wheels;
		this.Colour = Colour;
		this.ManufactureDate = ManufactureDate;
	};

}
