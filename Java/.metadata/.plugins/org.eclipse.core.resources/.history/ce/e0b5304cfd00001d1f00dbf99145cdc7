package garageChallenge;

public class Car extends Vehicle{
	
	// Attributes
	String Make;
	String Model;
	Boolean Automatic;
	
	// Class Constructors
	public Car() {
	};
	
	public Car(Integer VehicleID, Integer Wheels, String Colour, Integer ManufactureYear, String Make, String Model, Boolean Automatic) {
		super(VehicleID, Wheels, Colour, ManufactureYear);
		this.Make = Make;
		this.Model = Model;
		this.Automatic = Automatic;
	}
	
	// Getters and Setters
	public String getMake() {
		return Make;
	}
	
	public String getModel() {
		return Model;
	}
	
	public Boolean getAutomatic() {
		return Automatic;
	}
	
	public void setMake(String Make) {
		this.Make = Make;
	}
	
	public void setModel(String Model) {
		this.Model = Model;
	}
	
	public void getAutomatic(Boolean Automatic) {
		this.Automatic = Automatic;
	}
	

	@Override
	public Double getAge(Integer CurrentYear) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public Double costOfWork() {
		// TODO Auto-generated method stub
		return null;
	}

}
