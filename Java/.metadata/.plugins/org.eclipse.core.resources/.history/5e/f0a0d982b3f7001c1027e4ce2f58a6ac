package com.qa.helloworld;

public class EmilyTest {
	
	public static void functionName() {
		String message =  "This line informs Java to create a string called 'message' consisting of this text";
		System.out.Println(message);
	}
	
	public static void messageTwo () {
		String message2 = "Remember to create a second section defining main, aka telling the console to run the function";
		System.out.Println(message2); 
	}
	
	public static void main (String[] args) {
		functionName();
		messageTwo();
	}

}
