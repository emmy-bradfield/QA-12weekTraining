package liskovSubstitution;

public class Studio extends OneRoom {
    private String road;
	private int sqft;

	public Studio(String road, int sqft) {
    	this.road = road;
    	this.sqft = sqft;
    }

    @Override
    public void setSquareFootage(int sqft) {
        this.setSquareFootage(sqft);
    }
}
