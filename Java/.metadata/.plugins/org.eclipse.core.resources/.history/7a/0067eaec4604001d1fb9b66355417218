import org.apache.logging.log4j.LogManager;
import org.apache.logging.log4j.Logger;

public class LoggingDemo {
	
	private final Logger LOGGER = LogManager.getLogger(); 
	
	public void loggerFunctions() {
		LOGGER.trace("The URL you have provided for the localhost is not recognised.\n'jdbc:mysql://localhost:3306/' followed by your database names is usually best.\nIf this is still not working, check the SQL connector is in\nthis project's build path");
		LOGGER.debug("debug");
		LOGGER.info("info");
		LOGGER.warn("warn");
		LOGGER.error("error");
		LOGGER.fatal("fatal");
	}
}
