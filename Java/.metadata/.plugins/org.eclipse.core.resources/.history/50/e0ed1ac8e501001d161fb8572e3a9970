package com.qa.singleResponsibility;

public class Driver {
	private int mileage;

	// functionality
	public void newMilage(int milesDriven) {
		Car.setMileage(Car.getMileage()+milesDriven);
	}

	public int getMileage() {
		return mileage;
	}

	public void setMileage(int mileage) {
		this.mileage = mileage;
	}
}