package com.emily.helloWorld;

public class NamingConvention {
	public static interface descriptive {}
	public static void doing() {
		String variableNames = "aVariable";
		String tempVarNames = "t (temporary variable)";
		String constantsEnums = "CONSTANTS_AND_ENUMS";
		String genericTypeNames = "G (generic types)";
		String Exceptions = "Exception";
		String packageNames = "domanin.company.package";
		String acronyms = "IFOA = In Full Or as Acronym";
		System.out.println(variableNames);
		System.out.println(tempVarNames);
		System.out.println(constantsEnums);
		System.out.println(genericTypeNames);
		System.out.println(Exceptions);
		System.out.println(packageNames);
		System.out.println(acronyms);
	}
		
	public static void main (String[] args) {
		doing();
	}
}
